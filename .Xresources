URxvt.font: xft:DejaVuSansMono Nerd Font Mono:size=11
URxvt.perl-ext-common:      default,resize-font,clipboard,url-select,keyboard-select
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
!URxvt.foreground: #D9D9D9
URxvt.scrollBar: false
URxvt.url-select.launcher:  google-chrome-stable
URxvt.url-select.underline: true
URxvt.keysym.Control-n:           perl:url-select:select_next
URxvt.keysym.Control-Escape:      perl:keyboard-select:activate
URxvt.keysym.Control-s:           perl:keyboard-select:search
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.iso14755: false
URxvt.iso14755_52: false
!URxvt.background: #3F3F3F
! X colors.
! Generated by 'wal'
*foreground:        #e1e5eb
*background:        #16222b
*.foreground:       #e1e5eb
*.background:       #16222b
emacs*foreground:   #e1e5eb
emacs*background:   #16222b
URxvt*foreground:   #e1e5eb
XTerm*foreground:   #e1e5eb
UXTerm*foreground:  #e1e5eb
URxvt*background:   [70]#16222b
XTerm*background:   #16222b
UXTerm*background:  #16222b
URxvt*cursorColor:  #e1e5eb
XTerm*cursorColor:  #e1e5eb
UXTerm*cursorColor: #e1e5eb
URxvt*borderColor:  [70]#16222b

